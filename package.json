{
  "name": "syfunddashboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "scraper": "ts-node ./src/scraper/index.ts",
    "scraper-prod": "NODE_ENV=prod ts-node ./src/scraper/index.ts",
    "build-dev": "yarn tsc && yarn webpack --config src/client/webpack/config.dev.js",
    "build-prod": "yarn tsc && yarn webpack --config src/client/webpack/config.prod.js",
    "watch-client": "yarn webpack --watch --watch-poll --config src/client/webpack/config.dev.js",
    "watch-server": "yarn tsc -w",
    "server-dev": "yarn run build-dev && cp -a ./src/server/views/. ./build/server/views &&  yarn nodemon",
    "server-prod": "NODE_ENV=prod yarn run build-prod && cp -a ./src/server/views/. ./build/server/views && node ./build/server/server.js",
    "start": "NODE_ENV=prod node ./build/server/server.js",
    "heroku-postbuild": "yarn run build-prod && cp -a ./src/server/views/. ./build/server/views"
  },
  "engines": {
    "node": "10.x"
  },
  "repository": {
